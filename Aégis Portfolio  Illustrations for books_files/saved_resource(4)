$fullsize=jQuery('#fullsize');$scrollerItems=jQuery('#fullsize .item');$cntItems=$scrollerItems.length;$fullsizeTimer=jQuery('#fullsizeTimer');$fullsizeStart=jQuery('#fullsizeStart');$fullsizeStop=jQuery('#fullsizeStop');$togthumbs=jQuery('#toggleThumbnails');jQuery.fn.stopTimer=function(){jQuery(this).stop().width(0);$fullsizeStart.show();$fullsizeStop.hide();}
